<h1>Users#show_users</h1>
<p>Find me in app/views/users/show_users.html.erb</p>

<table border ="3">
	<thead>
		<th>First Name</th>
		<th>Last Name</th>
		<th>User Name</th>
		<th>Email</th>
	</thead>
	<tbody>
		<%@users.each do |u|%>
			<tr>
			<td><%= u.first_name%></td>
			<td><%= u.last_name%></td>
			<td><%= u.user_name%></td>
			<td><%= u.email%></td>
		</tr>
		<%end%>
	</tbody>
</table>
